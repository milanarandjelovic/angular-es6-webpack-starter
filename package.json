{
  "name": "angular-es6-webpack-starter",
  "version": "1.0.0",
  "description": "AngularJS starter kit, Webpack, ES6",
  "scripts": {
    "dev": "npm run development",
    "development": "webpack-dev-server --config webpack.config.dev.babel.js --port 9000 --inline --hot",
    "prod": "npm run production",
    "production": "webpack --config webpack.config.prod.babel.js --progress",
    "test-server": "http-server dist -p 9000 -a localhost"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6",
    "starter"
  ],
  "author": "Milan Arandjelovic",
  "repository": {
